<h1>Articles#index</h1>
<h2>Hello, Rails!</h2>
<p>Find me in app/views/articles/index.html.erb</p>

<ul>
	<% @articles.each do |article| %>
		<li>
			<article>
				<h3>
					<a href='/articles/<%= article.id %>'>
						<%= article.title %>
					</a>
				</h3>
				<br/>
				<p>
					<%= article.body %>
				</p>
			</article>
		</li>
	<% end %>
</ul>
